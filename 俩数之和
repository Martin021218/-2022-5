#include "pch.h"
#include <iostream>

int add(int x, int y)
{
    return x + y;
}

int x()
{
    int q{};
    std::cout << "a=";
    std::cin >> q;
    return q;
}

int y()
{
    int w{};
    std::cout << "b=";
    std::cin >> w;
    return w;
}


int main()
{
    int a{x()}; 
    int b{y()}; 

    std::cout <<"ans="<< add(a, b) << '\n';

    return 0;
}
